import pytest
from fuel import convert, gauge

def test_positive_convert():
    assert convert("1/4") == int("25")

def test_zero_division():
    with pytest.raises(ZeroDivisionError):
        assert convert("1/0")

def test_value_error():
    with pytest.raises(ValueError):
        assert convert("cat/dog")

def test_full_99():
    assert gauge(99) == "F"

def test_full_100():
    assert gauge(100) == "F"

def test_empty_0():
    assert gauge(0) == "E"

def test_empty_1():
    assert gauge(1) == "E"

def test_positive_gauge():
    assert gauge(25) == "25%"
